<Window x:Class="Main_Screen.TransportViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Main_Window"
        mc:Ignorable="d"
        Title="TransportViewWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Select Store Cleanup Date" HorizontalAlignment="Left" Margin="52,94,0,0" VerticalAlignment="Top"/>
        <DatePicker HorizontalAlignment="Left" Margin="75,125,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="lstEntries" Margin="442,189,0,0"/>
        <Label Content="Truck/trailer number" HorizontalAlignment="Left" Margin="67,181,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtTruck" HorizontalAlignment="Left" Margin="66,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="120"/>
        <Label Content="List of entries" HorizontalAlignment="Left" Margin="568,149,0,0" VerticalAlignment="Top" FontSize="18"/>
        <Label Content="Transport View" HorizontalAlignment="Left" Margin="272,4,0,0" VerticalAlignment="Top" FontSize="36"/>

    </Grid>
</Window>
